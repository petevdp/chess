{
  "extends": ["gts/tslint.json", "tslint-react"],
  "linterOptions": {
    "exclude": [
      "**/*.json",
      "node_modules/*"
    ]
  },
  "rules": {
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case"],
    "interface-name": false,
    "no-default-export": false,
    "jsx-boolean-value": [false]
  }
}
